{
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss public/tailwind.css -o public/build/tailwind.css --watch\"  \"live-server ./public\"",
    "development": "cross-env NODE_ENV=development postcss public/tailwind.css -o public/build/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss public/tailwind.css -o public/build/tailwind.css",
    "start": "npm run serve",
    "webpack": "webpack",
    "autre": "webpack-dev-server",
    "webpack_prod": "webpack --config=webpack.prod.js",
    "start_prod": "webpack-dev-server --config=webpack.prod.js"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "tailwindcss": "^1.0",
    "blend4web": "^17.12.0-dp.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss-cli": "^6.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
